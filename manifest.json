{
    "manifest_version": "3",
    "name": "brOkie",
    "version": "1.0",
    "description": "Cookie visualisation extension",
    "icons": {
        "128": "icon_128.png"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "content.js"
        ]
    },
    "permissions": [
        "http://*.google.com/",
        "https://*.google.com/"
    ],
    "browser_action": {
        "default_title": "",
        "default_icon": "icon_19.png",
        "default_popup": "popup.html"
    }
}